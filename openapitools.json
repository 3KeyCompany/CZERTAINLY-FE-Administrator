{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.2.1",
    "generators": {
      "typescript-rxjs-local": {
        "generatorName": "typescript-rxjs",
        "output": "./src/types/openapi",
        "glob": "./src/types/openapi/src/doc-openapi-czertainly-core.yaml",
        "typeMappings": {
          "set": "Array",
          "Date": "string"
        },
        "additionalProperties": {
          "withoutRuntimeChecks": true,
          "supportsES6": true,
          "withInterfaces": true,
          "stringEnums": true,
          "typescriptThreePlus": true
        }
      }
    }
  }
}
